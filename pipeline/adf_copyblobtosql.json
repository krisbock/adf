{
	"name": "adf_copyblobtosql",
	"properties": {
		"activities": [
			{
				"name": "Copy_kha",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "demo/inputEmp.txt"
					},
					{
						"name": "Destination",
						"value": "[dbo].[emp]"
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureBlobFSSource",
						"recursive": false
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"Prop_0": "FirstName",
							"Prop_1": "LastName"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_kha",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_kha",
						"type": "DatasetReference"
					}
				]
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}